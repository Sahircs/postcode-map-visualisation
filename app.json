{
  "expo": {
    "name": "postcode-map-visualisation",
    "slug": "postcode-map-visualisation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "description": "Mobile App that displays London Postcodes on a Map by Area and a Chart that displays the distribution of Postcodes by Area. \nUsed React-Native/Expo, React, Redux, TypeScript.",
    "githubUrl": "https://github.com/Sahircs/postcode-map-visualisation"
  }
}
